{
    "displayName":"3D Building - Custom Turbulent Inflow - Under Construction",
    "internalName":"customInflow",
    "description":"This model computes \nthe air flow around a building\n using an uploaded 3D geometry,\nusing customized inflows",
    "icon":"upload3D.png",
    "debugOnly":true,
    "disable":false,
    "list_order":3,
    "parent":"upload3D.json",
    "stages":[
        "mesh",
        {
            "displayName":"Simulation",
            "internalName":"sim",
            "app":"cwe-parallel",
            "groups":[
                {
                    "displayName":"Simulation Control",
                    "internalName":"simulation_control",
                    "vars":[
                        {
                            "displayName":"Simulation Duration",
                            "internalName":"endTime",
                            "type":"std",
                            "default":"1.0",
                            "unit":"s",
                            "precision":"2",
                            "sign":"+"
                        },
                        {
                            "displayName":"Simulation Time Step",
                            "internalName":"deltaT",
                            "type":"std",
                            "default":"0.1",
                            "unit":"s",
                            "precision":"6",
                            "sign":"+"
                        },
                        {
                            "displayName":"Kinematic Viscosity",
                            "internalName":"nu",
                            "type":"std",
                            "default":"1.48e-05",
                            "unit":"m^2/s",
                            "precision":"9",
                            "sign":"+"
                        }
                    ]
                },
                {
                    "displayName":"Inflow Conditions",
                    "internalName":"inflow",
                    "vars":[
                        {
                            "displayName":"Mean Velocity Reference Value",
                            "internalName":"velocity",
                            "type":"std",
                            "default":"1",
                            "unit":"m/s",
                            "precision":"6",
                            "sign":"+"
                        },
                        {
                            "displayName":"Inflow Distibution",
                            "internalName":"inflowDist",
                            "type":"choose",
                            "default":"uniform",
                            "options":{
                                "uniform":"Uniform Inflow",
                                "exponential":"Exponential Inflow Profile"
                            }
                        },
                        {
                            "displayName":"Inflow Type",
                            "internalName":"inflowChoose",
                            "type":"choose",
                            "default":"digitalFilter",
                            "options":{
                                "digitalFilter":"Digital Filter",
                                "syntheticEddy":"Synthetic Eddy"
                            },
                            "showCondition":"$inflowDist!uniform"
                        },
                        {
                            "displayName":"Eddy Density",
                            "internalName":"eddyDensity",
                            "type":"std",
                            "default":"0.1",
                            "precision":"2",
                            "sign":"+",
                            "showCondition":"$inflowChoose=syntheticEddy"
                        },
                        {
                            "displayName":"Inflow Shape",
                            "internalName":"eddyDensity",
                            "type":"choose",
                            "default":"gaussian",
                            "options":{
                                "tent":"Tent",
                                "step":"Step",
                                "gaussian":"Gaussian"
                            },
                            "showCondition":"$inflowChoose=syntheticEddy"
                        },
                        {
                            "displayName":"Digital Filter Shape",
                            "internalName":"filterShape",
                            "type":"choose",
                            "default":"gaussian",
                            "options":{
                                "exponential":"Exponential",
                                "gaussian":"Gaussian"
                            },
                            "showCondition":"$inflowChoose=digitalFilter"
                        },
                        {
                            "displayName":"Grid Factor",
                            "internalName":"gridFactor",
                            "type":"std",
                            "default":"1",
                            "precision":"0",
                            "sign":"+",
                            "showCondition":"$inflowChoose=digitalFilter"
                        },
                        {
                            "displayName":"Filter Factor",
                            "internalName":"filterFactor",
                            "type":"std",
                            "default":"8",
                            "precision":"0",
                            "sign":"+",
                            "showCondition":"$inflowChoose=digitalFilter"
                        }
                    ]
                },
                {
                    "displayName":"Advanced",
                    "internalName":"advanced",
                    "vars":[
                        {
                            "displayName":"Turbulence Model",
                            "internalName":"turbModel",
                            "type":"choose",
                            "default":"laminar",
                            "options":{
                                "laminar":"Laminar Flow model",
                                "SpalartAllmaras":"S-A one equation model (RANS)",
                                "kEpsilon":"k-epsilon model (RANS)",
                                "Smagorinsky":"Smagorinsky turbulence model (LES)",
                                "dynOneEqEddy":"Dynamic One Equation model (LES)",
                                "SpalartAllmarasDDES": "Spalart Allmaras DDES model"
                            }
                        },
                        {
                            "displayName":"Compute Force Coefficients?",
                            "internalName":"useForceCoeffs",
                            "type":"bool",
                            "default": true
                        },
                        {
                            "displayName":"Compute Mean Velocity Field?",
                            "internalName":"useFieldUmean",
                            "type":"bool",
                            "default": false
                        },
                        {
                            "displayName":"Compute Mean Pressure Field?",
                            "internalName":"useFieldPmean",
                            "type":"bool",
                            "default": false
                        },
                        {
                            "displayName":"Number of PISO Correctors",
                            "internalName":"pisoCorrectors",
                            "type":"std",
                            "default":"2",
                            "precision":"int",
                            "sign":"+0"
                        },
                        {
                            "displayName":"Number of non-orthogonal Correctors",
                            "internalName":"pisoNonOrthCorrect",
                            "type":"std",
                            "default":"1",
                            "precision":"int",
                            "sign":"+0"
                        },
                        {
                            "displayName":"Turbulence Intensity",
                            "internalName":"turbintensity",
                            "type":"std",
                            "default":"0.1",
                            "precision":"2",
                            "sign":"+",
                            "showCondition":"$turbModel!laminar"
                        }
                    ]
                }
            ],
            "results":[
                {
                    "displayName":"Sim Agave Log",
                    "type":"text",
                    "file":"logs/.agave.log"
                },
                {
                    "displayName":"Sim Error Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.err"
                },
                {
                    "displayName":"Sim Output Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.out"
                },
                {
                    "displayName":"OpenFOAM Log",
                    "type":"text",
                    "file":"logs/sim.log"
                },
                {
                    "displayName":"VTK Visualization Files",
                    "type":"download",
                    "file":"VTK.zip"
                }
            ]
        }
    ]
}
